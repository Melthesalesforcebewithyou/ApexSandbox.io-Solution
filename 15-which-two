public String whichTwo(Integer a, Integer b, Integer c) {
    // Check if a + b == c
        if (a + b == c) {
            return 'c';
        }
        // Check if a + c == b
        if (a + c == b) {
            return 'b';
        }
        // Check if b + c == a
        if (b + c == a) {
            return 'a';
        }
        // If no pairs add up to a third number, return an empty string
        return '';
    }